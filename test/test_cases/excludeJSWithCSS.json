{
	"htmlWebpackPlugin": {
	  "files": {
	    "css": [ "style.css" ],
	    "js": [ "head_bundle.js", "style.js" ],
	    "chunks": {
	      "head": {
	        "entry": "head_bundle.js",
	        "css": []
	      },
				"style": {
					"entry": "style.js",
					"css": [ "style.css" ]
				}
	    }
	  },
		"options": {
			"inline": "style",
			"excludeJSWithCSS": true
		}
	}
}
