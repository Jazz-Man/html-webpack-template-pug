include ../../mixins.pug

doctype html
html(lang='en')
	head
		meta(charset='utf-8')
		meta(http-equiv='X-UA-Compatible', content='IE=edge')
		base(href="../assets/page.html")
		
		+title
		//- inlines all .css files
		+inline(/\.css$/)

		block head

	body

		h3 htmlWebpackPlugin:
		pre= JSON.stringify(htmlWebpackPlugin, null, 2)
			
		//- inlines all .js files
		+inline(/\.js$/)
			
		block scripts
